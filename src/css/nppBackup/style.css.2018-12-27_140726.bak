
/*Additional*/
.tabel-shipping-gb{
margin:20px 0;	
}

.on-green{
color:green;
}

.off-red{
color:red;
font-weight:bold;
}

.tabel-shipping-gb tr.titledesc th {
    padding: 19px 12px;
    border: 1px solid #ddd;
    font-weight: bold;
}
.tabel-shipping-gb td {
    padding: 10px 12px;
    border-right: 1px solid #ddd;
}

.wrap .tabel-shipping-gb .action-button:hover {
    border-color: #008ec2;
    background: #00a0d2;
    color: #fff;
}

.wrap .tabel-shipping-gb .action-button
{
    padding: 4px 8px;
    position: relative;
    text-decoration: none;    
    border: 1px solid #ccc;
    border-radius: 2px;
    background: #f7f7f7;
    text-shadow: none;
    font-weight: 600;
    font-size: 13px;
    line-height: normal;
    color: #0073aa;
    cursor: pointer;
    outline: 0;
}

.wrap .tabel-shipping-gb .spinner {
    background: url(images/spinner.gif) no-repeat;
    background-size: 20px 20px;
    display: inline-block;
    /* visibility: hidden; */
    /* float: right; */
    vertical-align: middle;
    opacity: .7;
    filter: alpha(opacity=70);
    width: 20px;
    height: 20px;
    margin: 4px 10px 0;
}



